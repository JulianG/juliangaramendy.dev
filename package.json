{
  "name": "jg.dev",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "tsc:watch": "tsc --pretty --noEmit --watch",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@mapbox/rehype-prism": "^0.5.0",
    "@next/bundle-analyzer": "^10.0.4",
    "dayjs": "^1.9.6",
    "gray-matter": "^4.0.2",
    "mongodb": "^3.6.4",
    "next": "latest",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rehype-stringify": "^8.0.0",
    "remark-gfm": "^1.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.0.0",
    "sha-1": "^1.0.0",
    "ua-parser-js": "^0.7.24",
    "unified": "^9.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^10.0.1",
    "@types/jest": "^26.0.19",
    "@types/mongodb": "^3.6.6",
    "@types/node": "^13.9.5",
    "@types/react": "^16.9.27",
    "@types/ua-parser-js": "^0.7.35",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.0",
    "babel-jest": "^26.6.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.0.10",
    "prettier": "^2.0.2",
    "typescript": "^4.1.3"
  }
}
